(define (format-ascendant sign degree)
  ;; :: String â†’ Integer â†’ String
  ;;
  ;; format ascendant as "asc-{sign4}{deg2}" (exactly 10 chars).
  ;;
  ;; examples:
  ;;   sign="Aries", degree=15  â†’ "asc-arie15"
  ;;   sign="Leo", degree=23    â†’ "asc-leo023"
  ;;   sign="Pisces", degree=5  â†’ "asc-pisc05"
  ;;
  ;; the degree is always zero-padded to 2 digits.
  
  (string-append "asc-"
                 (abbreviate-sign sign)
                 (format "{:02}" degree)))

(define (format-sun-house house)
  ;; :: Integer â†’ String
  ;;
  ;; format sun house as "sun-{house2}h" (exactly 8 chars).
  ;;
  ;; examples:
  ;;   house=4  â†’ "sun-04h"
  ;;   house=10 â†’ "sun-10h"
  ;;   house=1  â†’ "sun-01h"
  ;;
  ;; the house number is always zero-padded to 2 digits, followed by 'h'.
  
  (format "sun-{:02}h" house))

(define (format-timezone tz-abbr)
  ;; :: String â†’ String
  ;;
  ;; format timezone as 3-char abbreviation.
  ;;
  ;; examples:
  ;;   "PDT"  â†’ "PDT"
