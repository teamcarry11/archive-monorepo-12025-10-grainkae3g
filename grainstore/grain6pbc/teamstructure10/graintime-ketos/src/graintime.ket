;;; main graintime generator
;;; â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

(define (generate-graintime-76 params)
  ;; :: Map â†’ String
  ;;
  ;; generate exactly 76-character graintime with perfect column alignment.
  ;;
  ;; parameters (as map):
  ;;   :year        â†’ integer (holocene calendar, e.g., 12025)
  ;;   :month       â†’ integer (1-12)
  ;;   :day         â†’ integer (1-31)
  ;;   :hour        â†’ integer (0-23)
  ;;   :minute      â†’ integer (0-59)
  ;;   :timezone    â†’ string (3-char abbreviation, e.g., "PDT")
  ;;   :nakshatra   â†’ string (full name, e.g., "Purva Ashadha")
  ;;   :asc-sign    â†’ string (e.g., "Leo")
  ;;   :asc-degree  â†’ integer (0-30)
  ;;   :sun-house   â†’ integer (1-12)
  ;;   :team-base   â†’ string (e.g., "structure", "descend")
  ;;
  ;; returns: string of EXACTLY 76 characters
  ;;
  ;; format: 12025-10-27--0145-PDT--moon-{nak13}asc-leo023-sun-04h{team17}
  ;;         â”œâ”€dateâ”€â”€â”¤  â”œtimeâ”¤ â”œtzâ”¤  â”œâ”€nakshatraâ”€â”¤â”œascendantâ”¤â”œhouseâ”€â”¤â”œâ”€teamâ”€â”€â”¤
  ;;         11 chars   4     3      18 chars      10        8        17
  ;;         Total: 11 + 4 + 3 + 18 + 10 + 8 + 17 + 5 dashes = 76 chars
  ;;
  ;; categorical semantics: this function is a natural transformation from the
  ;; category of temporal-astronomical data to the category of fixed-width strings.
  ;; it preserves temporal ordering while achieving spatial uniformity.
  
  (let* (;; extract parameters
         (year       (get params :year))
         (month      (get params :month))
         (day        (get params :day))
         (hour       (get params :hour))
         (minute     (get params :minute))
         (tz         (get params :timezone))
         (nakshatra  (get params :nakshatra))
         (asc-sign   (get params :asc-sign))
         (asc-degree (get params :asc-degree))
         (sun-house  (get params :sun-house))
         (team-base  (get params :team-base))
         
         ;; format components
         (nak-abbr     (abbreviate-nakshatra nakshatra))
         (nak-padded   (pad-nakshatra-to-13 nak-abbr))
         (asc-formatted (format-ascendant asc-sign asc-degree))
         (sun-formatted (format-sun-house sun-house))
         (team-padded  (pad-team-to-17 team-base)))
    
    ;; assemble the graintime
    ;; format string breakdown:
    ;;   {year}-{month:02}-{day:02}--{hour:02}{minute:02}-{tz}--moon-{nak}{asc}-{sun}{team}
    (format "{}-{:02}-{:02}--{:02}{:02}-{}--moon-{}{}-{}{}"
            year month day hour minute tz
            nak-padded asc-formatted sun-formatted team-padded)))

