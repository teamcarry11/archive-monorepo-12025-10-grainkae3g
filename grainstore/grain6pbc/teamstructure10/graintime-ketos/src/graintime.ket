(define (abbreviate-nakshatra full-name)
  ;; :: String â†’ String
  ;;
  ;; convert full nakshatra name to abbreviated form for 76-char format.
  ;; 
  ;; examples:
  ;;   "Purva Ashadha"     â†’ "p_ashadha"     (9 chars)
  ;;   "Uttara Bhadrapada" â†’ "u_bhadrapada"  (12 chars, longest)
  ;;   "Mula"              â†’ "mula"          (4 chars, shortest)
  ;;
  ;; algebraic property: this function is a monoid homomorphism from the
  ;; set of nakshatra names to the set of abbreviated strings. it preserves
  ;; the structure while compacting the representation.
  
  (or (get nakshatra-abbreviations full-name)
      (string-downcase full-name)))

