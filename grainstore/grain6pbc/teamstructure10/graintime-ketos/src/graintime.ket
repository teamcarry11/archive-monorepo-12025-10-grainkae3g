(define (validate-graintime-76 graintime-str)
  ;; :: String â†’ Map
  ;;
  ;; validate that a graintime string is exactly 76 characters.
  ;;
  ;; returns: {:valid? boolean, :length integer, :message string}
  ;;
  ;; this validator ensures structural rigidity. in ketos we use specs to
  ;; encode contracts but here we keep it simple with boolean validation.
  
  (let ((len (string-length graintime-str)))
    {:valid?  (equal? 76 len)
     :length  len
     :message (if (equal? 76 len)
                  "âœ… perfect 76-char graintime âœ§ï½¥ï¾Ÿ:*"
                  (format "âŒ length mismatch: expected 76, got {}" len))}))

