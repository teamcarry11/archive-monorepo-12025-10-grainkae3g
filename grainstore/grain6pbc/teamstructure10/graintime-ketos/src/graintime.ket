;;; padding functions (achieve perfect 76-char alignment)
;;; â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

(define (pad-nakshatra-to-13 nakshatra-abbr)
  ;; :: String â†’ String
  ;;
  ;; pad nakshatra abbreviation to exactly 13 characters with trailing dashes.
  ;;
  ;; the nakshatra section must be precisely 13 chars for vertical alignment:
  ;;   "u_bhadrapada" (12 chars) â†’ "u_bhadrapada-"  (12 + 1 = 13)
  ;;   "mula"         (4 chars)  â†’ "mula---------"  (4 + 9 = 13)
  ;;   "p_ashadha"    (9 chars)  â†’ "p_ashadha----"  (9 + 4 = 13)
  ;;
  ;; functorial property: this is a functor from (String, length) to (String, 13).
  ;; it lifts variable-width strings into the fixed-width space while preserving
  ;; their semantic content.
  
  (let ((padding-needed (- 13 (string-length nakshatra-abbr))))
    (string-append nakshatra-abbr
                   (make-string padding-needed #\-))))

(define (pad-team-to-17 team-base)
  ;; :: String â†’ String
  ;;
  ;; pad team name to exactly 17 characters with LEADING dashes.
  ;;
  ;; the team section must be precisely 17 chars for vertical alignment:
  ;;   "structure" â†’ "team" + "structure" + "10" = "teamstructure10" (15 chars)
  ;;                 15 chars needs 2 leading dashes â†’ "--teamstructure10" (17 total)
  ;;   "flow"      â†’ "team" + "flow" + "10" = "teamflow10" (10 chars)
  ;;                 10 chars needs 7 leading dashes â†’ "-------teamflow10" (17 total)
  ;;
  ;; the formula: total-dashes = 2 + (15 - length(full-team))
  ;;              where full-team = "team" + team-base + "10"
  ;;
  ;; comonadic property: this extracts context (team-base) and extends it with
  ;; padding to create a uniform coalgebraic structure across all teams.
  
  (let* ((full-team (string-append "team" team-base "10"))
         (padding-needed (- 15 (string-length full-team)))
         (total-dashes (+ 2 padding-needed)))
    (string-append (make-string total-dashes #\-)
                   full-team)))

